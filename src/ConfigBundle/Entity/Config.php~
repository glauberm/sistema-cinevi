<?php
namespace ConfigBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Entity
 * @ORM\Table(name="configurations")
 */
class Config
{
    /**
     * @ORM\Id
     * @ORM\Column(type="integer")
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    protected $id;

    /**
     * @ORM\Column(type="string")
     **/
    protected $type;

    /**
     * @ORM\Column(type="string")
     **/
    protected $key;

    /**
     * @ORM\Column(type="string")
     **/
    protected $value;


}
